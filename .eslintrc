{
    "root": true,
    "parserOptions": {
      "sourceType": "module",
      "parser": "babel-eslint"
    },
    "globals": {
        "axios": true
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "worker": true,
      "mocha": true,
      "phantomjs": true,
      "serviceworker": true
    },
    "extends": "eslint:recommended",
    "plugins": [
      "html"
    ],
    "rules": {
      "no-debugger": 0,
      "indent": ["warn", 2],
      "no-console": ["off"],
      "quotes": ["warn", "single"],
      "semi": ["error", "always"]
    }
}
